	public void powerOff() {
		System.out.println("키위폰 OFF");
	}
	public void volumeUp() {
		watch.volumeUp();
	}
	public void volumeDown() {
		watch.volumeDown();
	}
	public void initMethod() {
		System.out.println("객체를 초기화할 때 호출할 수 있는 메서드");
		// <bean>멤버변수 사용
