package test;

public class Client {
	public static void main(String[] args) {
		// Phone phone = new ApplePhone();
		// 앞에 생략 가능
		BeanFactory bf = new BeanFactory();
		Phone phone = (Phone)bf.getBean("apple"); 
		// 오브젝트로 나오기 떄문에 형번환
		phone.powerOn();
		phone.volumeUp();
		phone.volumeDown();
		phone.powerOff();
	}
}
